 <body>
<div class="navbar-giotto">


    <div class='flex1'>
        <ul class='list-inline-left'>
          <li class='text-list-dashboard'>Dashboard</li>
          <li class='text-list-left'>About us</li>
        </ul>
    </div>

    <div class='flex1'>
      <div class='logo-container'>
        <%=image_tag 'logo.png'%>
      </div>
    </div>


    <div class='flex1'>
        <ul class='list-inline-right'>
          <li class='text-list-right'>Search</li>
          <li class='text-list-right'>Account</li>
        </ul>
    </div>

</div>

  <div class= "row-container">

      <div class= lateral_menu>

            <ul class= lateral_menu_list>
              <li><i class="fas fa-user-plus lateral-menu-logo"></i>Manage your team</li>
              <li><i class="fas fa-bars lateral-menu-logo"></i>Furnitures</li>
              <li><i class="fas fa-gift lateral-menu-logo"></i>My package</li>
              <li><i class="fas fa-truck lateral-menu-logo"></i>My deliveries</li>
              <li><i class="fas fa-wallet lateral-menu-logo"></i>My wallet</li>
              <li><i class="far fa-file-alt lateral-menu-logo"></i>Invoices</li>
            </ul>

            <ul class= lateral_menu_list>
              <li><i class="fas fa-user-cog lateral-menu-logo"></i>Settings</li>
            </ul>

      </div>
          <div class= "index-list ">

      <div class="flex-item ">


      <div class="package_title">
        <h3>Your Team:</h3>
        <div class="separator"></div>
      </div>


              <div class="user-card user-empty">
  <img src="https://source.unsplash.com/900x900/?white"
  class="avatar-large avatar-empty " />
  <div class="notification-content ">
    <h3 class=""> <%= link_to "Add new user", new_user_invitation_path(params: { team_id:  current_user.team_id }) %>
    </h3>
  </div>
</div>


      <% @current_user_team.each do |user| %>
        <% next if user == current_user %>
        <div class="user-card">
  <img src="https://source.unsplash.com/900x900/?face,people
    " class="avatar-large avatat-pos" />
  <div class="notification-content">
    <p><small><%= user.email %></small></p>
    <p>Lorem ipsum dolor sit amet, <strong>consectetur</strong> adipisicing elit.</p>
  </div>

  <div class="notification-actions">
        <% @user_id = user.id %>
        <a class=""> <%= link_to "Profil", team_packages_teams_path(params: { user_id: user.id }) %>
        </a>
  </div>
</div>
<% end %>
    </div>
  </div>
 </body>

